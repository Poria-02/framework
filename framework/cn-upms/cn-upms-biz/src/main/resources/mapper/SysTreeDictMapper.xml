<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.upms.biz.mapper.SysTreeDictMapper">


	<select id="selectDicts" resultType="cn.upms.biz.api.entity.SysTreeDict">
		select *
		from sys_tree_dict
		where `type`=#{type}
		<if test="name != null">
			and name=#{name}
		</if>
		<if test="code != null">
			and code=#{code}
		</if>
		limit #{start},#{limit}
	</select>
	<select id="selectDictsCount" resultType="java.lang.Integer">
		select *
		from sys_tree_dict
		where `type`=#{type}
		<if test="name != null">
			and name=#{name}
		</if>
		<if test="code != null">
			and code=#{code}
		</if>
		limit #{start},#{limit}
	</select>
	<select id="findById" resultType="cn.upms.biz.api.entity.SysTreeDict">
        select *
        from sys_tree_dict
        where id=#{id}
    </select>

</mapper>